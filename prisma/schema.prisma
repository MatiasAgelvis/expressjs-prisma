// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id           Int @id
  title        String
  author       String
  createdAt    DateTime
  tags         String[]
}

model deletedPost {
  id           Int @id
}